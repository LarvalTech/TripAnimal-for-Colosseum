{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "badge_rewards_program/build/client",
  "installCommand": "npm install && cd badge_rewards_program && npm install",
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
